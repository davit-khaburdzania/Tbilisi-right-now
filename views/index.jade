extends layout

block content
  a.tag(href='/Tbilisi')= 'Tbilisi'
  a.tag(href='/batumi')= 'Batumi'
  a.tag(href='/london')= 'London'
  a.tag(href='/california')= 'California'
  a.tag(href='/newyork')= 'NewYork'
  a.tag(href='/barcelona')= 'Barcelona'
  a.tag(href='/sydney')= 'Sydney'
  a.tag(href='/beijing')= 'Beijing'
  p.clear
  h1= title
  #images
    each item in data
      -if(item && item.link && item.images && item.images.low_resolution && item.caption)
        a(href="#{item.link}", target="_blank")
          img(src="#{item.images.low_resolution.url}",title="#{item.caption.text}")
  #footer
    h3= "Davit Khaburdzania"